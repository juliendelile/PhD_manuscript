@article{Kafer:2007do,
author = {K{\"a}fer, Jos and Hayashi, Takashi and Mar{\'e}e, Athanasius F M and Carthew, Richard W and Graner, Fran{\c c}ois},
title = {{Cell adhesion and cortex contractility determine cell patterning in the Drosophila retina}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2007},
volume = {104},
