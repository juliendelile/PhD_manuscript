journal = {Current topics in developmental biology},
year = {2008},
volume = {81},
pages = {291--310},
affiliation = {Department of Anatomy and Developmental Biology, Kyoto University Graduate School of Medicine, Yoshida Konoe-chou, Sakyo-Ku 606-8501, Japan.},
