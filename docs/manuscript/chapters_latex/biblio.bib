@article{Thery:2006jj,
author = {Th{\'e}ry, Manuel and Bornens, Michel},
title = {{Cell shape and cell division.}},
journal = {Current opinion in cell biology},
year = {2006},
volume = {18},
number = {6},
pages = {648--657},
month = dec,
affiliation = {CEA, DSV, DRDC, Laboratoire Biopuces, Grenoble, F38054 France.},
doi = {10.1016/j.ceb.2006.10.001},
pmid = {17046223},
language = {English},
rating = {0},
date-added = {2009-03-06T14:54:23GMT+00:00},
date-modified = {2012-12-17T11:44:01GMT+00:00},
abstract = {The correlation between cell shape elongation and the orientation of the division axis described by early cell biologists is still used as a paradigm in developmental studies. However, analysis of early embryo development and tissue morphogenesis has highlighted the role of the spatial distribution of cortical cues able to guide spindle orientation. In vitro studies of cell division have revealed similar mechanisms. Recent data support the possibility that the orientation of cell division in mammalian cells is dominated by cell adhesion and the associated traction forces developed in interphase. Cell shape is a manifestation of these adhesive and tensional patterns. These patterns control the spatial distribution of cortical signals and thereby guide spindle orientation and daughter cell positioning. From these data, cell division appears to be a continuous transformation ensuring the maintenance of tissue mechanical integrity.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17046223&retmode=ref&cmd=prlinks},
local-url = {file://localhost(null)},
file = {{Théry 2006 Cell shape and cell division.pdf:/Users/Julien/Documents/Papers2/Articles/Théry/2006/Théry 2006 Cell shape and cell division.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.ceb.2006.10.001}}
}

