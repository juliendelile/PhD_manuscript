@article{Vargas:2011gn,
author = {Vargas, Diego A and Zaman, Muhammad H},
title = {{Computational Model for Migration of a Cell Cluster in Three-Dimensional Matrices}},
journal = {Annals of Biomedical Engineering},
year = {2011},
month = mar,
affiliation = {Department of Biomedical Engineering, Boston University, 44 Cummington Street, Boston, MA, USA.},
doi = {10.1007/s10439-011-0290-9},
