@article{Shaya:2011ea,
author = {Shaya, O and Sprinzak, D},
title = {{From Notch signaling to fine-grained patterning: Modeling meets experiments.}},
journal = {Current opinion in genetics {\&} development},
year = {2011},
volume = {21},
number = {6},
pages = {732--739},
month = dec,
affiliation = {Department of Biochemistry {\&} Molecular Biology, George S. Wise Faculty of Life Science, Tel-Aviv University, Ramat-Aviv 69978, Israel.},
