@article{Ulrich:2005jz,
author = {Ulrich, Florian and Krieg, Michael and Sch{\"o}tz, Eva-Maria and Link, Vinzenz and Castanon, Irinka and Schnabel, Viktor and Taubenberger, Anna and Mueller, Daniel and Puech, Pierre-Henri and Heisenberg, Carl-Philipp},
title = {{Wnt11 functions in gastrulation by controlling cell cohesion through Rab5c and E-cadherin.}},
journal = {Developmental cell},
year = {2005},
volume = {9},
number = {4},
pages = {555--564},
month = oct,
affiliation = {Max Planck Institute of Molecular Cell Biology and Genetics, 01307 Dresden, Germany.},
doi = {10.1016/j.devcel.2005.08.011},
pmid = {16198297},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-12-12T16:11:18GMT+00:00},
date-modified = {2013-01-09T03:01:48GMT+00:00},
abstract = {Wnt11 plays a central role in tissue morphogenesis during vertebrate gastrulation, but the molecular and cellular mechanisms by which Wnt11 exerts its effects remain poorly understood. Here, we show that Wnt11 functions during zebrafish gastrulation by regulating the cohesion of mesodermal and endodermal (mesendodermal) progenitor cells. Importantly, we demonstrate that Wnt11 activity in this process is mediated by the GTPase Rab5, a key regulator of early endocytosis, as blocking Rab5c activity in wild-type embryos phenocopies slb/wnt11 mutants, and enhancing Rab5c activity in slb/wnt11 mutant embryos rescues the mutant phenotype. In addition, we find that Wnt11 and Rab5c control the endocytosis of E-cadherin and are required in mesendodermal cells for E-cadherin-mediated cell cohesion. Together, our results suggest that Wnt11 controls tissue morphogenesis by modulating E-cadherin-mediated cell cohesion through Rab5c, a novel mechanism of Wnt signaling in gastrulation.},
url = {http://linkinghub.elsevier.com/retrieve/pii/S1534580705003059},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Ulrich/2005/Ulrich%202005%20Wnt11%20functions%20in%20gastrulation%20by%20controlling%20cell%20cohesion%20through%20Rab5c%20and%20E-cadherin.pdf},
file = {{Ulrich 2005 Wnt11 functions in gastrulation by controlling cell cohesion through Rab5c and E-cadherin.pdf:/Users/Julien/Documents/Papers2/Articles/Ulrich/2005/Ulrich 2005 Wnt11 functions in gastrulation by controlling cell cohesion through Rab5c and E-cadherin.pdf:application/pdf;Ulrich 2005 Wnt11 functions in gastrulation by controlling cell cohesion through Rab5c and E-cadherin.pdf:/Users/Julien/Documents/Papers2/Articles/Ulrich/2005/Ulrich 2005 Wnt11 functions in gastrulation by controlling cell cohesion through Rab5c and E-cadherin.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.devcel.2005.08.011}}
}

