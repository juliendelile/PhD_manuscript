volume = {136},
number = {8},
pages = {1305--1315},
month = apr,
annote = {3 to 5 mitosis
},
affiliation = {Max Planck Institute of Molecular Cell Biology and Genetics, Pfotenhauerstr. 108, 01307 Dresden, Germany.},
doi = {10.1242/dev.026922},
pmid = {19279138},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-03-16T10:41:20GMT+00:00},
date-modified = {2013-01-09T03:01:47GMT+00:00},
abstract = {Nuclear movements play an essential role in metazoan development. Although the intracellular transport mechanisms underlying nuclear movements have been studied in detail, relatively little is known about signals from surrounding cells and tissues controlling these movements. Here, we show that, in gastrulating zebrafish embryos, convergence movements of nuclei within the yolk syncytial layer (YSL) are guided by mesoderm and endoderm progenitors migrating along the surface of the yolk towards the dorsal side of the developing gastrula. Progenitor cells direct the convergence movements of internal yolk syncytial nuclei (iYSN) by modulating cortical flow within the YSL in which the iYSN are entrained. The effect of mesoderm and endoderm progenitors on the convergence movement of iYSN depends on the expression of E-cadherin, indicating that adhesive contact between the cells and the YSL is required for the mesendoderm-modulated YSL cortical flow mediating nuclear convergence. In summary, our data reveal a crucial function for cortical flow in the coordination of syncytial nuclear movements with surrounding cells and tissues during zebrafish gastrulation.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=19279138&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Carvalho/2009/Carvalho%202009%20Control%20of%20convergent%20yolk%20syncytial%20layer%20nuclear%20movement%20in%20zebrafish.pdf},
file = {{Carvalho 2009 Control of convergent yolk syncytial layer nuclear movement in zebrafish.pdf:/Users/Julien/Documents/Papers2/Articles/Carvalho/2009/Carvalho 2009 Control of convergent yolk syncytial layer nuclear movement in zebrafish.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1242/dev.026922}}
}

