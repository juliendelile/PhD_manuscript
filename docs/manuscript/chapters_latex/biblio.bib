file = {{Goonesinghe 2012 Desmosomal cadherins in zebrafish epiboly and gastrulation.pdf:/Users/Julien/Documents/Papers2/Articles/Goonesinghe/2012/Goonesinghe 2012 Desmosomal cadherins in zebrafish epiboly and gastrulation.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1186/1471-213X-12-1}}
}

