file = {{Ahringer 2003 Control of cell polarity and mitotic spindle positioning in animal cells-1.pdf:/Users/Julien/Documents/Papers2/Articles/Ahringer/2003/Ahringer 2003 Control of cell polarity and mitotic spindle positioning in animal cells-1.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/463CA446-D0C4-459F-BD27-EB2832F5D177}}
}

