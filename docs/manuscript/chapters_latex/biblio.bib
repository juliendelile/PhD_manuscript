@article{Discher:2005ft,
author = {Discher, Dennis E and Janmey, Paul and Wang, Yu-Li},
title = {{Tissue cells feel and respond to the stiffness of their substrate}},
journal = {Science},
year = {2005},
volume = {310},
number = {5751},
pages = {1139--1143},
month = nov,
affiliation = {School of Engineering and Applied Science and Cell and Molecular Biology Graduate Group, University of Pennsylvania, Philadelphia, PA 19104-6315, USA. discher@seas.upenn.edu},
doi = {10.1126/science.1116995},
pmid = {16293750},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2009-03-06T14:50:36GMT+00:00},
date-modified = {2012-12-17T11:43:40GMT+00:00},
abstract = {Normal tissue cells are generally not viable when suspended in a fluid and are therefore said to be anchorage dependent. Such cells must adhere to a solid, but a solid can be as rigid as glass or softer than a baby's skin. The behavior of some cells on soft materials is characteristic of important phenotypes; for example, cell growth on soft agar gels is used to identify cancer cells. However, an understanding of how tissue cells-including fibroblasts, myocytes, neurons, and other cell types-sense matrix stiffness is just emerging with quantitative studies of cells adhering to gels (or to other cells) with which elasticity can be tuned to approximate that of tissues. Key roles in molecular pathways are played by adhesion complexes and the actinmyosin cytoskeleton, whose contractile forces are transmitted through transcellular structures. The feedback of local matrix stiffness on cell state likely has important implications for development, differentiation, disease, and regeneration.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Discher/2005/Discher%202005%20Tissue%20cells%20feel%20and%20respond%20to%20the%20stiffness%20of%20their%20substrate.pdf},
file = {{Discher 2005 Tissue cells feel and respond to the stiffness of their substrate.pdf:/Users/Julien/Documents/Papers2/Articles/Discher/2005/Discher 2005 Tissue cells feel and respond to the stiffness of their substrate.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1126/science.1116995}}
}

