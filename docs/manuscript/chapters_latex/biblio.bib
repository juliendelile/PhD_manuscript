@article{Davidson:2008co,
author = {Davidson, Lance A and Dzamba, Bette D and Keller, Ray and Desimone, Douglas W},
title = {{Live imaging of cell protrusive activity, and extracellular matrix assembly and remodeling during morphogenesis in the frog, Xenopus laevis}},
journal = {Developmental dynamics : an official publication of the American Association of Anatomists},
year = {2008},
volume = {237},
number = {10},
pages = {2684--2692},
month = oct,
affiliation = {Department of Cell Biology, University of Virginia Health System, Charlottesville, Virginia, USA. lad43@pitt.edu},
doi = {10.1002/dvdy.21600},
pmid = {18629871},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2009-07-18T23:31:45GMT+00:00},
date-modified = {2012-12-17T11:43:36GMT+00:00},
abstract = {Cell motility and matrix assembly have traditionally been studied in isolation because of a lack of suitable model systems in which both can be observed simultaneously. With embryonic tissues from the gastrulating frog Xenopus laevis we observe stages of fibronectin fibrillogenesis coincident with protrusive activity in the overlying cells. Using live confocal time-lapse images collected from Cy3-tagged fibronectin and plasma membrane tethered green fluorescent protein, we describe the movement and the elaboration of a complex fibrillar network undergoing topological rearrangements of fibrils on the surface of an embryonic tissue. Discrete processes of annealing, polymerization, stretching, breaking, and recoiling are recorded. Elaboration and maintenance of the complex topology of the extracellular matrix appears to require filamentous actin. These findings support a mechanical-model in which cell tractive forces elaborate the complex topological fibrillar network and are part of a homeostatic mechanism for the regulation of the extracellular matrix.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Davidson/2008/Davidson%202008%20Live%20imaging%20of%20cell%20protrusive%20activity%20and%20extracellular%20matrix%20assembly%20and%20remodeling%20during%20morphogenesis%20in%20the%20frog%20Xenopus%20laevis.pdf},
file = {{Davidson 2008 Live imaging of cell protrusive activity and extracellular matrix assembly and remodeling during morphogenesis in the frog Xenopus laevis.pdf:/Users/Julien/Documents/Papers2/Articles/Davidson/2008/Davidson 2008 Live imaging of cell protrusive activity and extracellular matrix assembly and remodeling during morphogenesis in the frog Xenopus laevis.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/dvdy.21600}}
}

@article{Kimmel:1995kn,
author = {Kimmel, C B and Ballard, W W and Kimmel, S R and Ullmann, B and Schilling, T F},
title = {{Stages of embryonic development of the zebrafish}},
journal = {Developmental dynamics : an official publication of the American Association of Anatomists},
year = {1995},
volume = {203},
number = {3},
pages = {253--310},
month = jul,
affiliation = {Institute of Neuroscience, University of Oregon, Eugene 97403-1254, USA.},
doi = {10.1002/aja.1002030302},
pmid = {8589427},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2009-07-20T09:18:55GMT+00:00},
date-modified = {2013-01-09T03:01:47GMT+00:00},
abstract = {We describe a series of stages for development of the embryo of the zebrafish, Danio (Brachydanio) rerio. We define seven broad periods of embryogenesis--the zygote, cleavage, blastula, gastrula, segmentation, pharyngula, and hatching periods. These divisions highlight the changing spectrum of major developmental processes that occur during the first 3 days after fertilization, and we review some of what is known about morphogenesis and other significant events that occur during each of the periods. Stages subdivide the periods. Stages are named, not numbered as in most other series, providing for flexibility and continued evolution of the staging series as we learn more about development in this species. The stages, and their names, are based on morphological features, generally readily identified by examination of the live embryo with the dissecting stereomicroscope. The descriptions also fully utilize the optical transparancy of the live embryo, which provides for visibility of even very deep structures when the embryo is examined with the compound microscope and Nomarski interference contrast illumination. Photomicrographs and composite camera lucida line drawings characterize the stages pictorially. Other figures chart the development of distinctive characters used as staging aid signposts.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Kimmel/1995/Kimmel%201995%20Stages%20of%20embryonic%20development%20of%20the%20zebrafish.pdf},
file = {{Kimmel 1995 Stages of embryonic development of the zebrafish.pdf:/Users/Julien/Documents/Papers2/Articles/Kimmel/1995/Kimmel 1995 Stages of embryonic development of the zebrafish.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/aja.1002030302}}
}

