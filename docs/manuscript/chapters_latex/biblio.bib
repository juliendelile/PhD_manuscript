uri = {\url{papers2://publication/doi/10.1126/science.121467}}
}

@article{Kierzkowski:2012jv,
author = {Kierzkowski, Daniel and Nakayama, Naomi and Routier-Kierzkowska, Anne-Lise and Weber, Alain and Bayer, Emmanuelle and Schorderet, Martine and Reinhardt, Didier and Kuhlemeier, Cris and Smitherties, Richard S},
title = {{Elastic domains regulate growth and organogenesis in the plant shoot apical meristem}},
journal = {Science},
year = {2012},
volume = {335},
number = {1096},
pages = {1--5},
month = mar,
doi = {10.1126/science.1213100},
rating = {0},
date-added = {2012-03-06T19:04:33GMT+00:00},
date-modified = {2012-11-05T20:37:34GMT+00:00},
abstract = {Although genetic control of morphogenesis is well established, elaboration of complex shapes requires changes in the mechanical properties of cells. In plants, the first visible sign of leaf formation is a bulge on the flank of the shoot apical meristem. Bulging results from local relaxation of cell walls, which causes them to yield to internal hydrostatic pressure. By manipulation of tissue tension in combination with quantitative live imaging and finite-element modeling, we found that the slow-growing area at the shoot tip is substantially strain-stiffened compared with surrounding fast-growing tissue. We propose that strain siffening limist growth, restricts organ bulging, and contributes to the meristem's functional zonation. Thus, mechanical signals are not just passive readoust of gene action but feed back on morphogenesis.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Kierzkowski/2012/Kierzkowski%202012%20Elastic%20domains%20regulate%20growth%20and%20organogenesis%20in%20the%20plant%20shoot%20apical%20meristem.pdf},
file = {{Kierzkowski 2012 Elastic domains regulate growth and organogenesis in the plant shoot apical meristem.pdf:/Users/Julien/Documents/Papers2/Articles/Kierzkowski/2012/Kierzkowski 2012 Elastic domains regulate growth and organogenesis in the plant shoot apical meristem.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1126/science.1213100}}
}

