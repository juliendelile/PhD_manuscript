}

@article{Placinta:2009il,
author = {Placinta, Mike and Shen, Meng-Chieh and Achermann, Marc and Karlstrom, Rolf O},
title = {{A laser pointer driven microheater for precise local heating and conditional gene regulation in vivo}},
journal = {BMC developmental biology},
year = {2009},
volume = {9},
number = {1},
pages = {73},
month = dec,
doi = {10.1186/1471-213X-9-73},
pmid = {20042114},
language = {ENG},
read = {Yes},
rating = {0},
date-added = {2010-01-08T09:40:56GMT+00:00},
date-modified = {2012-12-17T11:43:34GMT+00:00},
abstract = {ABSTRACT: BACKGROUND: Tissue heating has been employed to study a variety of biological processes, including the study of genes that control embryonic development. Conditional regulation of gene expression is a particularly powerful approach for understanding gene function. One popular method for mis-expressing a gene of interest employs heat-inducible heat shock protein (hsp) promoters. Global heat shock of hsp-promoter-containing transgenic animals induces gene expression throughout all tissues, but does not allow for spatial control. Local heating allows for spatial control of hsp-promoter-driven transgenes, but methods for local heating are cumbersome and variably effective. RESULTS: We describe a simple, highly controllable, and versatile apparatus for heating biological tissue and other materials on the micron-scale. This microheater employs micron-scale fiber optics and uses an inexpensive laser-pointer as a power source. Optical fibers can be pulled on a standard electrode puller to produce tips of varying sizes that can then be used to reliably heat 20-100 um targets. We demonstrate precise spatiotemporal control of hsp70l:GFP transgene expression in a variety of tissue types in zebrafish embryos and larvae. We also show how this system can be employed as part of a new method for lineage tracing that would greatly facilitate the study of organogenesis and tissue regulation at any time in the life cycle. CONCLUSION: This versatile and simple local heater has broad utility for the study of gene function and for lineage tracing. This system could be used to control hsp-driven gene expression in any organism simply by bringing the fiber optic tip in contact with the tissue of interest. Beyond these uses for the study of gene function, this device has wide-ranging utility in materials science and could easily be adapted for therapeutic purposes in humans.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Placinta/2009/Placinta%202009%20A%20laser%20pointer%20driven%20microheater%20for%20precise%20local%20heating%20and%20conditional%20gene%20regulation%20in%20vivo.pdf},
file = {{Placinta 2009 A laser pointer driven microheater for precise local heating and conditional gene regulation in vivo.pdf:/Users/Julien/Documents/Papers2/Articles/Placinta/2009/Placinta 2009 A laser pointer driven microheater for precise local heating and conditional gene regulation in vivo.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1186/1471-213X-9-73}}
}

