@article{Blaser:2006hz,
author = {Blaser, Heiko and Reichman-Fried, Michal and Castanon, Irinka and Dumstrei, Karin and Marlow, Florence L and Kawakami, Koichi and Solnica-Krezel, Lilianna and Heisenberg, Carl-Philipp and Raz, Erez},
title = {{Migration of zebrafish primordial germ cells: a role for myosin contraction and cytoplasmic flow.}},
journal = {Developmental cell},
year = {2006},
volume = {11},
number = {5},
pages = {613--627},
month = nov,
affiliation = {Germ Cell Development, Max Planck Institute for Biophysical Chemistry, Am Fassberg 11, 37077 G{\"o}ttingen, Germany.},
doi = {10.1016/j.devcel.2006.09.023},
pmid = {17084355},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-03-06T14:51:02GMT+00:00},
date-modified = {2013-01-09T03:01:47GMT+00:00},
abstract = {The molecular and cellular mechanisms governing cell motility and directed migration in response to the chemokine SDF-1 are largely unknown. Here, we demonstrate that zebrafish primordial germ cells whose migration is guided by SDF-1 generate bleb-like protrusions that are powered by cytoplasmic flow. Protrusions are formed at sites of higher levels of free calcium where activation of myosin contraction occurs. Separation of the acto-myosin cortex from the plasma membrane at these sites is followed by a flow of cytoplasm into the forming bleb. We propose that polarized activation of the receptor CXCR4 leads to a rise in free calcium that in turn activates myosin contraction in the part of the cell responding to higher levels of the ligand SDF-1. The biased formation of new protrusions in a particular region of the cell in response to SDF-1 defines the leading edge and the direction of cell migration.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17084355&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Blaser/2006/Blaser%202006%20Migration%20of%20zebrafish%20primordial%20germ%20cells%20a%20role%20for%20myosin%20contraction%20and%20cytoplasmic%20flow.pdf},
file = {{Blaser 2006 Migration of zebrafish primordial germ cells a role for myosin contraction and cytoplasmic flow.pdf:/Users/Julien/Documents/Papers2/Articles/Blaser/2006/Blaser 2006 Migration of zebrafish primordial germ cells a role for myosin contraction and cytoplasmic flow.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.devcel.2006.09.023}}
}

