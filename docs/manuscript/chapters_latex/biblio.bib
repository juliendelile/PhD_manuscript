@article{Mayer:2010kt,
author = {Mayer, Mirjam and Depken, Martin and Bois, Justin S and J{\"u}licher, Frank and Grill, Stephan W},
title = {{Anisotropies in cortical tension reveal the physical basis of polarizing cortical flows.}},
journal = {Nature},
year = {2010},
volume = {467},
number = {7315},
pages = {617--621},
month = sep,
affiliation = {Max Planck Institute of Molecular Cell Biology and Genetics, 01307 Dresden, Germany.},
doi = {10.1038/nature09376},
pmid = {20852613},
language = {English},
rating = {0},
date-added = {2012-08-29T08:17:57GMT+00:00},
date-modified = {2012-12-17T11:43:23GMT+00:00},
abstract = {Asymmetric cell divisions are essential for the development of multicellular organisms. To proceed, they require an initially symmetric cell to polarize. In Caenorhabditis elegans zygotes, anteroposterior polarization is facilitated by a large-scale flow of the actomyosin cortex, which directs the asymmetry of the first mitotic division. Cortical flows appear in many contexts of development, but their underlying forces and physical principles remain poorly understood. How actomyosin contractility and cortical tension interact to generate large-scale flow is unclear. Here we report on the subcellular distribution of cortical tension in the polarizing C. elegans zygote, which we determined using position- and direction-sensitive laser ablation. We demonstrate that cortical flow is associated with anisotropies in cortical tension and is not driven by gradients in cortical tension, which contradicts previous proposals. These experiments, in conjunction with a theoretical description of active cortical mechanics, identify two prerequisites for large-scale cortical flow: a gradient in actomyosin contractility to drive flow and a sufficiently large viscosity of the cortex to allow flow to be long-ranged. We thus reveal the physical requirements of large-scale intracellular cortical flow that ensure the efficient polarization of the C. elegans zygote.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20852613&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Mayer/2010/Mayer%202010%20Anisotropies%20in%20cortical%20tension%20reveal%20the%20physical%20basis%20of%20polarizing%20cortical%20flows.pdf},
file = {{Mayer 2010 Anisotropies in cortical tension reveal the physical basis of polarizing cortical flows.pdf:/Users/Julien/Documents/Papers2/Articles/Mayer/2010/Mayer 2010 Anisotropies in cortical tension reveal the physical basis of polarizing cortical flows.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nature09376}}
}

