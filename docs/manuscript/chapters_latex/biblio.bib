@article{Martin:2010je,
author = {Martin, Adam C and Gelbart, Michael and Fernandez-Gonzalez, Rodrigo and Kaschube, Matthias and Wieschaus, Eric F},
title = {{Integration of contractile forces during tissue invagination}},
journal = {The Journal of cell biology},
year = {2010},
volume = {188},
number = {5},
pages = {735--749},
month = mar,
affiliation = {Department of Molecular Biology, Howard Hughes Medical Institute, Princeton University, Princeton, NJ 08544, USA.},
doi = {10.1083/jcb.200910099},
pmid = {20194639},
language = {eng},
rating = {0},
date-added = {2010-10-17T19:35:30GMT+00:00},
date-modified = {2012-11-05T20:37:34GMT+00:00},
abstract = {Contractile forces generated by the actomyosin cytoskeleton within individual cells collectively generate tissue-level force during epithelial morphogenesis. During Drosophila mesoderm invagination, pulsed actomyosin meshwork contractions and a ratchet-like stabilization of cell shape drive apical constriction. Here, we investigate how contractile forces are integrated across the tissue. Reducing adherens junction (AJ) levels or ablating actomyosin meshworks causes tissue-wide epithelial tears, which release tension that is predominantly oriented along the anterior-posterior (a-p) embryonic axis. Epithelial tears allow cells normally elongated along the a-p axis to constrict isotropically, which suggests that apical constriction generates anisotropic epithelial tension that feeds back to control cell shape. Epithelial tension requires the transcription factor Twist, which stabilizes apical myosin II, promoting the formation of a supracellular actomyosin meshwork in which radial actomyosin fibers are joined end-to-end at spot AJs. Thus, pulsed actomyosin contractions require a supracellular, tensile meshwork to transmit cellular forces to the tissue level during morphogenesis.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Martin/2010/Martin%202010%20Integration%20of%20contractile%20forces%20during%20tissue%20invagination.pdf},
file = {{Martin 2010 Integration of contractile forces during tissue invagination.pdf:/Users/Julien/Documents/Papers2/Articles/Martin/2010/Martin 2010 Integration of contractile forces during tissue invagination.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1083/jcb.200910099}}
}

@article{Chang:2010jj,
