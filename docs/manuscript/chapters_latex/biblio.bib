uri = {\url{papers2://publication/doi/10.1242/dev.046987}}
}

@article{Skoglund:2010fl,
author = {Skoglund, Paul and Keller, Ray},
title = {{Integration of planar cell polarity and ECM signaling in elongation of the vertebrate body plan}},
journal = {Current opinion in cell biology},
year = {2010},
volume = {22},
number = {5},
pages = {589--596},
month = oct,
affiliation = {Department of Biology, University of Virginia, Charlottesville, VA 22904, USA.},
