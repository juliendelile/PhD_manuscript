@article{Weber:2011hi,
author = {Weber, Gregory F and Bjerke, Maureen A and Desimone, Douglas W},
title = {{A mechanoresponsive cadherin-keratin complex directs polarized protrusive behavior and collective cell migration.}},
journal = {Developmental cell},
year = {2012},
volume = {22},
number = {1},
pages = {104--115},
month = jan,
affiliation = {Department of Cell Biology, School of Medicine, University of Virginia Health System, Charlottesville, VA 22908, USA.},
doi = {10.1016/j.devcel.2011.10.013},
pmid = {22169071},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-12-12T11:39:25GMT+00:00},
date-modified = {2012-11-05T20:37:34GMT+00:00},
abstract = {Collective cell migration requires maintenance of adhesive contacts between adjacent cells, coordination of polarized cell protrusions, and generation of propulsive traction forces. We demonstrate that mechanical force applied locally to C-cadherins on single Xenopus mesendoderm cells is sufficient to induce polarized cell protrusion and persistent migration typical of individual cells within a collectively migrating tissue. Local tension on cadherin adhesions induces reorganization of the keratin intermediate filament network toward these stressed sites. Plakoglobin, a member of the catenin family, is localized to cadherin adhesions under tension and is required for both mechanoresponsive cell behavior and assembly of the keratin cytoskeleton at the rear of these cells. Local tugging forces on cadherins occur in vivo through interactions with neighboring cells, and these forces result in coordinate changes in cell protrusive behavior. Thus, cadherin-dependent force-inducible regulation of cell polarity in single mesendoderm cells represents an emergent property of the intact tissue.},
url = {http://dx.doi.org/10.1016/j.devcel.2011.10.013},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Weber/2012/Weber%202012%20A%20mechanoresponsive%20cadherin-keratin%20complex%20directs%20polarized%20protrusive%20behavior%20and%20collective%20cell%20migration.pdf},
file = {{Weber 2012 A mechanoresponsive cadherin-keratin complex directs polarized protrusive behavior and collective cell migration.pdf:/Users/Julien/Documents/Papers2/Articles/Weber/2012/Weber 2012 A mechanoresponsive cadherin-keratin complex directs polarized protrusive behavior and collective cell migration.pdf:application/pdf;Weber 2012 A mechanoresponsive cadherin-keratin complex directs polarized protrusive behavior and collective cell migration.pdf:/Users/Julien/Documents/Papers2/Articles/Weber/2012/Weber 2012 A mechanoresponsive cadherin-keratin complex directs polarized protrusive behavior and collective cell migration.pdf:application/pdf}},
