title = {{The EGF-CFC protein one-eyed pinhead is essential for nodal signaling.}},
journal = {Cell},
year = {1999},
volume = {97},
number = {1},
pages = {121--132},
month = apr,
affiliation = {Skirball Institute of Biomolecular Medicine, Department of Cell Biology, New York University School of Medicine, New York 10016, USA.},
