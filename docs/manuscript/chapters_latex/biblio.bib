@article{Pantazis:2010jb,
author = {Pantazis, Periklis and Maloney, James and Wu, David and Fraser, Scott E},
title = {{Second harmonic generating (SHG) nanoprobes for in vivo imaging}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2010},
volume = {107},
number = {33},
pages = {14535--14540},
month = aug,
affiliation = {Division of Biology, California Institute of Technology, Beckman Institute 139-74, Pasadena, CA 91125, USA. pantazis@caltech.edu},
doi = {10.1073/pnas.1004748107},
pmid = {20668245},
language = {eng},
rating = {0},
date-added = {2010-10-18T09:33:41GMT+00:00},
date-modified = {2012-12-17T11:43:52GMT+00:00},
abstract = {Fluorescence microscopy has profoundly changed cell and molecular biology studies by permitting tagged gene products to be followed as they function and interact. The ability of a fluorescent dye to absorb and emit light of different wavelengths allows it to generate startling contrast that, in the best cases, can permit single molecule detection and tracking. However, in many experimental settings, fluorescent probes fall short of their potential due to dye bleaching, dye signal saturation, and tissue autofluorescence. Here, we demonstrate that second harmonic generating (SHG) nanoprobes can be used for in vivo imaging, circumventing many of the limitations of classical fluorescence probes. Under intense illumination, such as at the focus of a laser-scanning microscope, these SHG nanocrystals convert two photons into one photon of half the wavelength; thus, when imaged by conventional two-photon microscopy, SHG nanoprobes appear to generate a signal with an inverse Stokes shift like a fluorescent dye, but with a narrower emission. Unlike commonly used fluorescent probes, SHG nanoprobes neither bleach nor blink, and the signal they generate does not saturate with increasing illumination intensity. The resulting contrast and detectability of SHG nanoprobes provide unique advantages for molecular imaging of living cells and tissues.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Pantazis/2010/Pantazis%202010%20Second%20harmonic%20generating%20(SHG)%20nanoprobes%20for%20in%20vivo%20imaging.pdf},
file = {{Pantazis 2010 Second harmonic generating (SHG) nanoprobes for in vivo imaging.pdf:/Users/Julien/Documents/Papers2/Articles/Pantazis/2010/Pantazis 2010 Second harmonic generating (SHG) nanoprobes for in vivo imaging.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.1004748107}}
}

@article{Siddiqui:2010ik,
author = {Siddiqui, Manal and Sheikh, Hasan and Tran, Christopher and Bruce, Ashley E E},
title = {{The tight junction component Claudin E is required for zebrafish epiboly}},
journal = {Developmental dynamics : an official publication of the American Association of Anatomists},
year = {2010},
volume = {239},
number = {2},
pages = {715--722},
month = feb,
affiliation = {Department of Cell and Systems Biology, University of Toronto, Toronto, Ontario, Canada.},
