file = {{Sugiyama 2009 Illuminating cell-cycle progression in the developing zebrafish embryo.pdf:/Users/Julien/Documents/Papers2/Articles/Sugiyama/2009/Sugiyama 2009 Illuminating cell-cycle progression in the developing zebrafish embryo.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1073/pnas.0906464106}}
}

