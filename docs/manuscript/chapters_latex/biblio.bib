@article{SalazarCiudad:2005hy,
author = {Salazar-Ciudad, Isaac and Jernvall, Jukka},
title = {{Graduality and innovation in the evolution of complex phenotypes: insights from development}},
journal = {Journal of experimental zoology Part B Molecular and developmental evolution},
year = {2005},
volume = {304},
number = {6},
pages = {619--631},
month = nov,
affiliation = {Developmental Biology Program, Institute of Biotechnology, FIN-00014, University of Helsinki, Helsinki, Finland. isaac.salazar@helsinki.fi},
doi = {10.1002/jez.b.21058},
pmid = {16032700},
language = {eng},
rating = {0},
date-added = {2009-03-06T14:54:07GMT+00:00},
date-modified = {2012-12-17T11:44:04GMT+00:00},
abstract = {The neo-Darwinian paradigm benefits from the assumption that phenotypic variation is gradual and that phenotype and genotype have a relatively simple relationship. These assumptions are historically inherited from the times of the neo-Darwinian synthesis and, consequently, do not include present understanding about development. In this study, understanding about the dynamics of pattern formation is used to explore to that extent phenotypic variation can be expected to be gradual and simply related to molecular variation. Variation in simple phenotypes seems to fit neo-Darwinian assumptions but variation in complex phenotypes does not. Instead, variation in complex phenotypes would have a tendency to relatively less gradual evolution, even at microevolutionary time scales, that would make phylogenetic reconstructions more difficult. In addition, they will have a tendency to exhibit specific trends in innovation rates over group radiations with early accelerations and late decelerations. This work also explores further consequences of these results in our understanding of phenotypic evolution.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Salazar-Ciudad/2005/Salazar-Ciudad%202005%20Graduality%20and%20innovation%20in%20the%20evolution%20of%20complex%20phenotypes%20insights%20from%20development.pdf},
file = {{Salazar-Ciudad 2005 Graduality and innovation in the evolution of complex phenotypes insights from development.pdf:/Users/Julien/Documents/Papers2/Articles/Salazar-Ciudad/2005/Salazar-Ciudad 2005 Graduality and innovation in the evolution of complex phenotypes insights from development.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1002/jez.b.21058}}
}

