@article{Ewald:2008fe,
author = {Ewald, Andrew J and Brenot, Audrey and Duong, Myhanh and Chan, Bianca S and Werb, Zena},
title = {{Collective epithelial migration and cell rearrangements drive mammary branching morphogenesis}},
journal = {Developmental cell},
year = {2008},
volume = {14},
number = {4},
pages = {570--581},
month = apr,
affiliation = {Department of Anatomy and Program in Cell Biology, University of California, San Francisco, San Francisco, CA 94143, USA. andrew.ewald@ucsf.edu},
