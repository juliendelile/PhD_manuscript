@article{Fanto:2000wi,
author = {Fanto, M and Weber, U and Strutt, D I and Mlodzik, M},
title = {{Nuclear signaling by Rac and Rho GTPases is required in the establishment of epithelial planar polarity in the Drosophila eye}},
journal = {Current biology : CB},
year = {2000},
volume = {10},
number = {16},
pages = {979--988},
month = aug,
affiliation = {EMBL, Developmental Biology Programme, Heidelberg, Germany.},
pmid = {10985385},
language = {eng},
rating = {0},
date-added = {2009-03-06T14:52:17GMT+00:00},
date-modified = {2012-12-17T11:44:11GMT+00:00},
abstract = {BACKGROUND: The small GTPases Rac and Rho act as cellular switches in many important biological processes. In the fruit fly Drosophila, RhoA participates in the establishment of planar polarity, a process mediated by the receptor Frizzled (Fz). Thus far, analysis of Rac in this process has not been possible because of the absence of mutant Rac alleles. Here, we have investigated the role of Rac and Rho in establishing the polarity of ommatidia in the Drosophila eye. RESULTS: By expressing a dominant negative or a constitutively activated form of Rac1, we interfered specifically with Rac signaling and disrupted ommatidial polarity. The resulting defects were similar to the loss/gain-of-function phenotypes typical of tissue-polarity genes. Through genetic interaction and rescue experiments involving a polarity-specific, loss-of-function dishevelled (dsh) allele, we found that Rac1 acts downstream of Dsh in the Fz signaling pathway, but upstream of, or in parallel to, RhoA. Rac signaled to the nucleus through the Jun N-terminal kinase (JNK) cascade in this process. By generating point mutations in the effector loop of RhoA, we found that RhoA also signals to the nucleus during the establishment of ommatidial polarity. Nevertheless, Rac and RhoA activated transcription of distinct target genes. CONCLUSIONS: Rac is specifically required downstream of Dsh in the Fz pathway. It functions upstream or in parallel to RhoA and both signal to the nucleus, through distinct effectors, to establish planar polarity in the Drosophila eye.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Fanto/2000/Fanto%202000%20Nuclear%20signaling%20by%20Rac%20and%20Rho%20GTPases%20is%20required%20in%20the%20establishment%20of%20epithelial%20planar%20polarity%20in%20the%20Drosophila%20eye.pdf},
file = {{Fanto 2000 Nuclear signaling by Rac and Rho GTPases is required in the establishment of epithelial planar polarity in the Drosophila eye.pdf:/Users/Julien/Documents/Papers2/Articles/Fanto/2000/Fanto 2000 Nuclear signaling by Rac and Rho GTPases is required in the establishment of epithelial planar polarity in the Drosophila eye.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/28C86F52-A974-4210-A5E5-130DBBDA9CFA}}
}

