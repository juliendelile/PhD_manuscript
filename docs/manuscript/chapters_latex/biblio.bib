@article{Krieg:2008jd,
author = {Krieg, M and Arboleda-Estudillo, Y and Puech, P-H and K{\"a}fer, J and Graner, F and M{\"u}ller, D J and Heisenberg, C-P},
title = {{Tensile forces govern germ-layer organization in zebrafish.}},
journal = {Nature Cell Biology},
year = {2008},
volume = {10},
number = {4},
pages = {429--436},
month = apr,
affiliation = {BIOTEC, Technische Universit{\"a}t Dresden, Tatzberg 47-51, 01307 Dresden, Germany.},
doi = {10.1038/ncb1705},
pmid = {18364700},
language = {English},
read = {Yes},
rating = {0},
date-added = {2009-03-06T14:52:33GMT+00:00},
date-modified = {2013-01-09T03:01:47GMT+00:00},
abstract = {Understanding the factors that direct tissue organization during development is one of the most fundamental goals in developmental biology. Various hypotheses explain cell sorting and tissue organization on the basis of the adhesive and mechanical properties of the constituent cells. However, validating these hypotheses has been difficult due to the lack of appropriate tools to measure these parameters. Here we use atomic force microscopy (AFM) to quantify the adhesive and mechanical properties of individual ectoderm, mesoderm and endoderm progenitor cells from gastrulating zebrafish embryos. Combining these data with tissue self-assembly in vitro and the sorting behaviour of progenitors in vivo, we have shown that differential actomyosin-dependent cell-cortex tension, regulated by Nodal/TGFbeta-signalling (transforming growth factor beta), constitutes a key factor that directs progenitor-cell sorting. These results demonstrate a previously unrecognized role for Nodal-controlled cell-cortex tension in germ-layer organization during gastrulation.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=18364700&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Krieg/2008/Krieg%202008%20Tensile%20forces%20govern%20germ-layer%20organization%20in%20zebrafish.pdf},
file = {{Krieg 2008 Tensile forces govern germ-layer organization in zebrafish.pdf:/Users/Julien/Documents/Papers2/Articles/Krieg/2008/Krieg 2008 Tensile forces govern germ-layer organization in zebrafish.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/ncb1705}}
}

