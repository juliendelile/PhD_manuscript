title = {{Mathematical modeling of planar cell polarity to understand domineering nonautonomy.}},
journal = {Science},
year = {2005},
volume = {307},
number = {5708},
pages = {423--426},
month = jan,
affiliation = {Department of Aeronautics and Astronautics, Stanford University, Stanford, CA 94305-4035, USA.},
