@article{Djiane:2000ve,
author = {Djiane, A and Riou, J and Umbhauer, M and Boucaut, J and Shi, D},
title = {{Role of frizzled 7 in the regulation of convergent extension movements during gastrulation in Xenopus laevis}},
journal = {Development},
year = {2000},
volume = {127},
number = {14},
pages = {3091--3100},
month = jul,
affiliation = {Groupe de Biologie Exp{\'e}rimentale, Laboratoire de Biologie Mol{\'e}culaire et Cellulaire du D{\'e}veloppement, CNRS UMR 7622, Universit{\'e} Pierre et Marie Curie, 75005 Paris, France.},
pmid = {10862746},
language = {eng},
rating = {0},
date-added = {2009-03-06T14:52:11GMT+00:00},
date-modified = {2012-12-17T11:44:11GMT+00:00},
abstract = {Wnt signalling plays a crucial role in the control of morphogenetic movements. We describe the expression and functional analyses of frizzled 7 (Xfz7) during gastrulation in Xenopus. Low levels of Xfz7 transcripts are expressed maternally during cleavage stages; its zygotic expression strongly increases at the beginning of gastrulation and is predominantly localized to the presumptive neuroectoderm and deep cells of the involuting mesoderm. Overexpression of Xfz7 in the dorsal equatorial region affects the movements of convergent extension and delays mesodermal involution. It alters the correct localization, but not the expression, of mesodermal and neural markers. These effects can be rescued by extra-Xfz7, which is a secreted form of the receptor that also weakly inhibits convergent extension when overexpressed. This suggests that the wild-type and truncated receptors have opposing effects when coexpressed and that overexpression of Xfz7 causes an increased signalling activity. Consistent with this, Xfz7 biochemically and functionally interacts with Xwnt11. In addition, Dishevelled, but not ({\&}bgr;)-catenin, synergizes with Xfz7 to affect convergent extension. Furthermore, overexpression of Xfz7 and Xwnt11 also affects convergent extension in activin-treated animal caps, and this can be efficiently reversed by coexpression of Cdc42(T17N), a dominant negative mutant of the small GTPase Cdc42 known as a key regulator of actin cytoskeleton. Conversely, Cdc42(G12V), a constitutively active mutant, rescues the effects of extra-Xfz7 on convergent extension in a dose-dependent manner. That both gain-of-function and loss-of-function of both frizzled and dishevelled produce the same phenotype has been well described in Drosophila tissue polarity. Therefore, our results suggest an endogenous role of Xfz7 in the regulation of convergent extension during gastrulation.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Djiane/2000/Djiane%202000%20Role%20of%20frizzled%207%20in%20the%20regulation%20of%20convergent%20extension%20movements%20during%20gastrulation%20in%20Xenopus%20laevis.pdf},
file = {{Djiane 2000 Role of frizzled 7 in the regulation of convergent extension movements during gastrulation in Xenopus laevis.pdf:/Users/Julien/Documents/Papers2/Articles/Djiane/2000/Djiane 2000 Role of frizzled 7 in the regulation of convergent extension movements during gastrulation in Xenopus laevis.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/190D7B6F-281E-4E2D-A7A1-CFFF1C248C9E}}
}

