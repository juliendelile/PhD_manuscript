title = {{Fast, high-contrast imaging of animal development with scanned light sheet-based structured-illumination microscopy}},
journal = {Nature Methods},
year = {2010},
volume = {7},
number = {8},
pages = {637--642},
month = aug,
affiliation = {Cell Biology and Biophysics Unit, European Molecular Biology Laboratory, Heidelberg, Germany. kellerp@janelia.hhmi.org},
