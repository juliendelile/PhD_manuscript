@article{Fink:2011bx,
author = {Fink, Jenny and Carpi, Nicolas and Betz, Timo and B{\'e}tard, Angelique and Chebah, Meriem and Azioune, Ammar and Bornens, Michel and Sykes, Cecile and Fetler, Luc and Cuvelier, Damien and Piel, Matthieu},
