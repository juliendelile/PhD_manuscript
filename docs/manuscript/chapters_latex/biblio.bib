@article{Eaton:1995vm,
author = {Eaton, S and Auvinen, P and Luo, L and Jan, Y N and Simons, K},
title = {{CDC42 and Rac1 control different actin-dependent processes in the Drosophila wing disc epithelium}},
journal = {The Journal of cell biology},
year = {1995},
volume = {131},
number = {1},
pages = {151--164},
month = oct,
affiliation = {European Cell Biology Laboratory, Cell Biology Programme, Heidelberg, Germany.},
pmid = {7559772},
language = {eng},
rating = {0},
date-added = {2009-03-06T14:52:14GMT+00:00},
date-modified = {2012-12-17T11:44:18GMT+00:00},
abstract = {Cdc42 and Rac1 are members of the rho family of small guanosinetriphosphatases and are required for a diverse set of cytoskeleton-membrane interactions in different cell types. Here we show that these two proteins contribute differently to the organization of epithelial cells in the Drosophila wing imaginal disc. Drac1 is required to assemble actin at adherens junctions. Failure of adherens junction actin assembly in Drac1 dominant-negative mutants is associated with increased cell death. Dcdc42, on the other hand, is required for processes that involve polarized cell shape changes during both pupal and larval development. In the third larval instar, Dcdc42 is required for apico-basal epithelial elongation. Whereas normal wing disc epithelial cells increase in height more than twofold during the third instar, cells that express a dominant-negative version of Dcdc42 remain short and are abnormally shaped. Dcdc42 localizes to both apical and basal regions of the cell during these events, and mediates elongation, at least in part, by effecting a reorganization of the basal actin cytoskeleton. These observations suggest that a common cdc42-based mechanism may govern polarized cell shape changes in a wide variety of cell types.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Eaton/1995/Eaton%201995%20CDC42%20and%20Rac1%20control%20different%20actin-dependent%20processes%20in%20the%20Drosophila%20wing%20disc%20epithelium.pdf},
file = {{Eaton 1995 CDC42 and Rac1 control different actin-dependent processes in the Drosophila wing disc epithelium.pdf:/Users/Julien/Documents/Papers2/Articles/Eaton/1995/Eaton 1995 CDC42 and Rac1 control different actin-dependent processes in the Drosophila wing disc epithelium.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/463026DD-3393-425A-ADC4-4AC787F070D5}}
}

