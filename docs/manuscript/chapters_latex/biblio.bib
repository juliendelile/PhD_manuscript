abstract = {Somitogenesis, the sequential formation of a periodic pattern along the antero-posterior axis of vertebrate embryos, is one of the most obvious examples of the segmental patterning processes that take place during embryogenesis and also one of the major unresolved events in developmental biology. In this article, we develop a mathematical formulation of a new version of the Clock and Wavefront model proposed by Pourqui{\'e} and co-workers (Dubrulle, J., McGrew, M.J., Pourqui{\'e}, O., 2001. FGF signalling controls somite boundary position and regulates segmentation clock control of spatiotemporal Hox gene activation. Cell 106, 219-232). Dynamic expression of FGF8 in the presomitic mesoderm constitutes the wavefront of determination which sweeps along the body axis interacting as it moves with the segmentation clock to gate cells into somites. We also show that the model can mimic the anomalies formed when progression of the wavefront is disturbed and make some experimental predictions that can be used to test the hypotheses underlying the model.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=16546158&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Baker/2006/Baker%202006%20A%20clock%20and%20wavefront%20mechanism%20for%20somite%20formation.pdf},
file = {{Baker 2006 A clock and wavefront mechanism for somite formation.pdf:/Users/Julien/Documents/Papers2/Articles/Baker/2006/Baker 2006 A clock and wavefront mechanism for somite formation.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.ydbio.2006.01.018}}
}

@article{Davies:1984gv,
