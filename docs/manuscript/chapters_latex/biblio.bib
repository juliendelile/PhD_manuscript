@article{Balguid:2008eo,
author = {Balguid, Angelique and Driessen, Niels J B and Mol, Anita and Schmitz, Joep P J and Verheyen, Fons and Bouten, Carlijn V C and Baaijens, Frank P T},
title = {{Stress related collagen ultrastructure in human aortic valves--implications for tissue engineering}},
journal = {Journal of biomechanics},
year = {2008},
volume = {41},
number = {12},
pages = {2612--2617},
month = aug,
affiliation = {Department of Biomedical Engineering, Eindhoven University of Technology, PO Box 513, 5600 MB Eindhoven, The Netherlands. a.balguid@tue.nl},
doi = {10.1016/j.jbiomech.2008.06.031},
pmid = {18701107},
language = {eng},
rating = {0},
date-added = {2009-03-06T14:50:58GMT+00:00},
date-modified = {2012-12-17T11:43:57GMT+00:00},
abstract = {Understanding the response of tissue structures to mechanical stress is crucial for optimization of mechanical conditioning protocols in the field of heart valve tissue engineering. In heart valve tissue, it is unclear to what extent mechanical loading affects the collagen fibril morphology. To determine if local stress affects the collagen fibril morphology, in terms of fibril diameter, its distribution, and the fibril density, this was investigated in adult native human aortic valve leaflets. Transmission electron microscopy images of collagen fibrils were analyzed at three locations: the commissures, the belly, and the fixed edge of the leaflets. Subsequently, the mechanical behavior of human aortic valves was used in a computational model to predict the stress distribution in the valve leaflet during the diastolic phase of the cardiac cycle. The local stresses at the three locations were related to the collagen fibril morphology. The fibril diameter and density varied significantly between the measured locations, and appeared inversely related. The average fibril diameter increased from the fixed edge, to the belly, and to the commissures of the leaflets, while fibril density decreased. Interestingly, these differences corresponded well with the level of stress at the locations. The presented data showed that large tissue stress is associated with greater average fibril diameter, lower fibril density, and wider fibril size distribution compared with low stress locations in the leaflets. The findings here provide insight in the effect of mechanical loading on the collagen ultrastructure, and are valuable to improve conditioning protocols for tissue engineering.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Balguid/2008/Balguid%202008%20Stress%20related%20collagen%20ultrastructure%20in%20human%20aortic%20valves--implications%20for%20tissue%20engineering.pdf},
file = {{Balguid 2008 Stress related collagen ultrastructure in human aortic valves--implications for tissue engineering.pdf:/Users/Julien/Documents/Papers2/Articles/Balguid/2008/Balguid 2008 Stress related collagen ultrastructure in human aortic valves--implications for tissue engineering.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.jbiomech.2008.06.031}}
}

