@article{Howley:2000uv,
author = {Howley, C and Ho, R K},
title = {{mRNA localization patterns in zebrafish oocytes}},
journal = {Mechanisms of Development},
year = {2000},
volume = {92},
number = {2},
pages = {305--309},
month = apr,
affiliation = {Department of Molecular Biology, Princeton University, Princeton, NJ 08544, USA. chowley@molbio.princeton.edu},
pmid = {10727871},
language = {eng},
rating = {0},
date-added = {2009-03-16T17:44:19GMT+00:00},
date-modified = {2012-12-17T11:44:11GMT+00:00},
abstract = {In both invertebrate and vertebrate systems, the localization of maternal mRNAs is a common mechanism used to influence developmental processes, including the establishment of the dorsal/ventral axis, anterior/posterior axis, and the germ line (for review, see Bashirullah et al., 1998. Annu. Rev. Biochem. 67, 335-394). While the existence of localized maternal mRNAs has been reported in the zebrafish, Danio rerio, the precise localization patterns of these molecules during oogenesis has not been determined. In this study, in situ hybridization experiments were performed on zebrafish ovaries and activated eggs to examine different mRNA localization patterns. The results establish that while some maternal mRNAs remain ubiquitously distributed throughout the oocyte, other mRNAs follow specific localization patterns, including localization to the animal pole, localization to the vegetal pole, and cortical localization. The animal/vegetal axis is first apparent in stage II oocytes when the earliest mRNA localization is seen. Unique patterns of localization are seen in mature eggs as well. Some mRNAs maintain their oocyte localization patterns, while others localize upon egg activation (fertilization).},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Howley/2000/Howley%202000%20mRNA%20localization%20patterns%20in%20zebrafish%20oocytes.pdf},
file = {{Howley 2000 mRNA localization patterns in zebrafish oocytes.pdf:/Users/Julien/Documents/Papers2/Articles/Howley/2000/Howley 2000 mRNA localization patterns in zebrafish oocytes.pdf:application/pdf}},
uri = {\url{papers2://publication/uuid/836A35D1-E3DA-4149-9CF1-7031FA4E7E43}}
}

@article{Montell:2008hy,
author = {Montell, Denise J},
title = {{Morphogenetic cell movements: diversity from modular mechanical properties}},
journal = {Science},
year = {2008},
volume = {322},
number = {5907},
pages = {1502--1505},
month = dec,
affiliation = {Department of Biological Chemistry, Center for Cell Dynamics, Rangos Building, Suite 450, 855 North Wolfe Street, Baltimore, MD 21205, USA. dmontell@jhmi.edu},
doi = {10.1126/science.1164073},
pmid = {19056976},
language = {eng},
rating = {0},
date-added = {2009-03-17T20:35:41GMT+00:00},
date-modified = {2012-12-17T11:43:57GMT+00:00},
abstract = {Animal tissue and organ development requires the orchestration of cell movements, including those of interconnected cell groups, termed collective cell movements. Such movements are incredibly diverse. Recent work suggests that two core cellular properties, cell-cell adhesion and contractility, can largely determine geometry, packing, sorting, and rearrangement of epithelial cell layers. Two additional force-generating properties, the ability to generate cell protrusions and cell adhesion to the extracellular matrix, contribute to active motility. These mechanical properties can be regulated independently in cells, suggesting that they can be employed in a combinatorial manner. A small number of properties used in combination could, in principle, generate a diverse array of cell shapes and arrangements and thus orchestrate the varied morphogenetic events observed during metazoan organ development.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Montell/2008/Montell%202008%20Morphogenetic%20cell%20movements%20diversity%20from%20modular%20mechanical%20properties.pdf},
file = {{Montell 2008 Morphogenetic cell movements diversity from modular mechanical properties.pdf:/Users/Julien/Documents/Papers2/Articles/Montell/2008/Montell 2008 Morphogenetic cell movements diversity from modular mechanical properties.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1126/science.1164073}}
}

@article{Cox:1998cg,
author = {Cox, E A and Huttenlocher, A},
title = {{Regulation of integrin-mediated adhesion during cell migration}},
