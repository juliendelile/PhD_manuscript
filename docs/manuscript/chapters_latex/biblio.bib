@article{Kimmel:1985vc,
author = {Kimmel, C B and Law, R D},
title = {{Cell lineage of zebrafish blastomeres. III. Clonal analyses of the blastula and gastrula stages.}},
journal = {Developmental Biology},
year = {1985},
volume = {108},
number = {1},
pages = {94--101},
month = mar,
pmid = {3972184},
language = {English},
rating = {0},
