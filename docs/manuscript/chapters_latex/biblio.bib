@article{Rejniak:2008ho,
author = {Rejniak, Katarzyna A and Anderson, Alexander R A},
title = {{A computational study of the development of epithelial acini: II. Necessary conditions for structure and lumen stability.}},
journal = {Bulletin of Mathematical Biology},
year = {2008},
volume = {70},
number = {5},
pages = {1450--1479},
month = jul,
affiliation = {Division of Mathematics, University of Dundee, Dundee, DD1 4HN, Scotland, UK. rejniak@maths.dundee.ac.uk},
doi = {10.1007/s11538-008-9308-3},
pmid = {18401665},
language = {English},
rating = {0},
date-added = {2012-08-28T16:30:51GMT+00:00},
date-modified = {2012-12-17T11:43:37GMT+00:00},
abstract = {Simple epithelial tissues are organized as single layers of tightly packed cells that surround hollow lumens and form selective barriers separating different internal compartments of the body. The maintenance of epithelial structure and its function requires tight coordination and control of all the life processes of epithelial cells via cell-to-cell communication and signaling. These well-balanced cellular systems are, however, quite often disturbed by genetic or environmental cues that may lead to the formation of epithelial tumors (carcinomas). In fact, more than a half of all diagnosed tumors are initiated from epithelial cells. It is, therefore, important to gain a greater understanding of the factors that form and maintain the epithelial structure, as well as the features of the acinar structure that are modified during cancer development as observable in experimental and clinical research. We address these questions using the bio-mechanical model of the developing hollow epithelial acini introduced in Rejniak and Anderson (Bull. Math. Biol. 70:677-712, 2008). Here, we propose several scenarios involving various bio-mechanical interactions between neighboring cells that result in abnormal acinar development. Whenever possible, we compare our computational results with known experimental cases of mutant acini.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=18401665&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Rejniak/2008/Rejniak%202008%20A%20computational%20study%20of%20the%20development%20of%20epithelial%20acini%20II.%20Necessary%20conditions%20for%20structure%20and%20lumen%20stability.pdf},
file = {{Rejniak 2008 A computational study of the development of epithelial acini II. Necessary conditions for structure and lumen stability.pdf:/Users/Julien/Documents/Papers2/Articles/Rejniak/2008/Rejniak 2008 A computational study of the development of epithelial acini II. Necessary conditions for structure and lumen stability.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1007/s11538-008-9308-3}}
}

@article{Rejniak:2008ij,
author = {Rejniak, Katarzyna A and Anderson, Alexander R A},
title = {{A computational study of the development of epithelial acini: I. Sufficient conditions for the formation of a hollow structure.}},
journal = {Bulletin of Mathematical Biology},
year = {2008},
volume = {70},
number = {3},
pages = {677--712},
month = apr,
affiliation = {Division of Mathematics, University of Dundee, Dundee, DD1 4HN, Scotland, UK. rejniak@maths.dundee.ac.uk},
doi = {10.1007/s11538-007-9274-1},
pmid = {18188652},
language = {English},
read = {Yes},
rating = {0},
date-added = {2012-08-28T16:32:02GMT+00:00},
date-modified = {2012-12-17T11:43:20GMT+00:00},
abstract = {Normal hollow epithelial acini are 3-dimensional culture structures that resemble the architecture and functions of normal breast glands and lobules. This experimental model enables in vitro investigations of genotypic and molecular abnormalities associated with epithelial cancers. However, the way in which the acinar structure is formed is not yet completely understood. Gaining more information about consecutive stages of acini development-starting from a single cell that gives rise to a cluster of randomly oriented cells, followed by cell differentiation that leads to a layer of polarised cells enclosing the hollow lumen-will provide insight into the transformations of eukaryotic cells that are necessary for their successful arrangement into an epithelium. In this paper, we introduce a two-dimensional single-cell-based model representing the cross section of a typical acinus. Using this model, we investigate mechanisms that lead to the unpolarised cell growth, cell polarisation, stabilisation of the acinar structure and maintenance of the hollow lumen and discuss the sufficient conditions for each stage of acinar formation. In the follow-up paper (Rejniak and Anderson, A computational study of the development of epithelial acini. II. Necessary conditions for structure and lumen stability), we investigate what morphological changes are observable in the growing acini when some assumptions of this model are relaxed.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=18188652&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Rejniak/2008/Rejniak%202008%20A%20computational%20study%20of%20the%20development%20of%20epithelial%20acini%20I.%20Sufficient%20conditions%20for%20the%20formation%20of%20a%20hollow%20structure.pdf},
file = {{Rejniak 2008 A computational study of the development of epithelial acini I. Sufficient conditions for the formation of a hollow structure.pdf:/Users/Julien/Documents/Papers2/Articles/Rejniak/2008/Rejniak 2008 A computational study of the development of epithelial acini I. Sufficient conditions for the formation of a hollow structure.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1007/s11538-007-9274-1}}
}

