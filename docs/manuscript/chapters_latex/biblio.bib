@article{Dupont:2032ez,
author = {Dupont, Sirio and Morsut, Leonardo and Aragona, Mariaceleste and Enzo, Elena and Giulitti, Stefano and Cordenonsi, Michelangelo and Zanconato, Francesca and Le Digabel, Jimmy and Forcato, Mattia and Bicciato, Silvio and Elvassore, Nicola and Piccolo, Stefano},
title = {{Role of YAP/TAZ in mechanotransduction.}},
journal = {Nature},
year = {2011},
volume = {474},
number = {7350},
pages = {179--183},
month = jun,
affiliation = {Department of Histology, Microbiology and Medical Biotechnologies, University of Padua School of Medicine, viale Colombo 3, 35131 Padua, Italy. dupont@bio.unipd.it},
doi = {10.1038/nature10137},
pmid = {21654799},
language = {English},
rating = {0},
date-added = {2011-06-09T08:53:09GMT+00:00},
date-modified = {2012-12-17T11:43:50GMT+00:00},
abstract = {Cells perceive their microenvironment not only through soluble signals but also through physical and mechanical cues, such as extracellular matrix (ECM) stiffness or confined adhesiveness. By mechanotransduction systems, cells translate these stimuli into biochemical signals controlling multiple aspects of cell behaviour, including growth, differentiation and cancer malignant progression, but how rigidity mechanosensing is ultimately linked to activity of nuclear transcription factors remains poorly understood. Here we report the identification of the Yorkie-homologues YAP (Yes-associated protein) and TAZ (transcriptional coactivator with PDZ-binding motif, also known as WWTR1) as nuclear relays of mechanical signals exerted by ECM rigidity and cell shape. This regulation requires Rho GTPase activity and tension of the actomyosin cytoskeleton, but is independent of the Hippo/LATS cascade. Crucially, YAP/TAZ are functionally required for differentiation of mesenchymal stem cells induced by ECM stiffness and for survival of endothelial cells regulated by cell geometry; conversely, expression of activated YAP overrules physical constraints in dictating cell behaviour. These findings identify YAP/TAZ as sensors and mediators of mechanical cues instructed by the cellular microenvironment.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=21654799&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Dupont/2011/Dupont%202011%20Role%20of%20YAPTAZ%20in%20mechanotransduction.pdf},
file = {{Dupont 2011 Role of YAPTAZ in mechanotransduction.pdf:/Users/Julien/Documents/Papers2/Articles/Dupont/2011/Dupont 2011 Role of YAPTAZ in mechanotransduction.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nature10137}}
}

@article{RiedelKruse:2007hh,
author = {Riedel-Kruse, Ingmar H and M{\"u}ller, Claudia and Oates, Andrew C},
title = {{Synchrony dynamics during initiation, failure, and rescue of the segmentation clock.}},
journal = {Science},
year = {2007},
volume = {317},
number = {5846},
pages = {1911--1915},
month = sep,
affiliation = {Max Planck Institute of Molecular Cell Biology and Genetics (MPI-CBG), Pfotenhauerstrasse 108, 01307 Dresden, Germany. ingmar@caltech.edu},
doi = {10.1126/science.1142538},
pmid = {17702912},
language = {English},
read = {Yes},
rating = {0},
date-added = {2011-06-15T13:45:07GMT+00:00},
date-modified = {2012-12-17T11:43:20GMT+00:00},
abstract = {The "segmentation clock" is thought to coordinate sequential segmentation of the body axis in vertebrate embryos. This clock comprises a multicellular genetic network of synchronized oscillators, coupled by intercellular Delta-Notch signaling. How this synchrony is established and how its loss determines the position of segmentation defects in Delta and Notch mutants are unknown. We analyzed the clock's synchrony dynamics by varying strength and timing of Notch coupling in zebra-fish embryos with techniques for quantitative perturbation of gene function. We developed a physical theory based on coupled phase oscillators explaining the observed onset and rescue of segmentation defects, the clock's robustness against developmental noise, and a critical point beyond which synchrony decays. We conclude that synchrony among these genetic oscillators can be established by simultaneous initiation and self-organization and that the segmentation defect position is determined by the difference between coupling strength and noise.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=17702912&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Riedel-Kruse/2007/Riedel-Kruse%202007%20Synchrony%20dynamics%20during%20initiation%20failure%20and%20rescue%20of%20the%20segmentation%20clock.pdf},
file = {{Riedel-Kruse 2007 Synchrony dynamics during initiation failure and rescue of the segmentation clock.pdf:/Users/Julien/Documents/Papers2/Articles/Riedel-Kruse/2007/Riedel-Kruse 2007 Synchrony dynamics during initiation failure and rescue of the segmentation clock.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1126/science.1142538}}
}

