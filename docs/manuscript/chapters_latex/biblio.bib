pages = {18549--18554},
month = nov,
affiliation = {Laboratoire de Spectrom{\'e}trie Physique, Unit{\'e} Mixte de Recherche 5588, Universit{\'e} Joseph-Fourier Grenoble I and Centre National de la Recherche Scientifique, 140 Avenue de la Physique, 38402 Saint Martin d'H{\`e}res, France. jkafer@spectro.ujf-grenoble.fr},
doi = {10.1073/pnas.0704235104},
pmid = {18003929},
