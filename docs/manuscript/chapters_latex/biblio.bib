date-modified = {2012-12-17T11:43:33GMT+00:00},
abstract = {Recording light-microscopy images of large, nontransparent specimens, such as developing multicellular organisms, is complicated by decreased contrast resulting from light scattering. Early zebrafish development can be captured by standard light-sheet microscopy, but new imaging strategies are required to obtain high-quality data of late development or of less transparent organisms. We combined digital scanned laser light-sheet fluorescence microscopy with incoherent structured-illumination microscopy (DSLM-SI) and created structured-illumination patterns with continuously adjustable frequencies. Our method discriminates the specimen-related scattered background from signal fluorescence, thereby removing out-of-focus light and optimizing the contrast of in-focus structures. DSLM-SI provides rapid control of the illumination pattern, exceptional imaging quality and high imaging speeds. We performed long-term imaging of zebrafish development for 58 h and fast multiple-view imaging of early Drosophila melanogaster development. We reconstructed cell positions over time from the Drosophila DSLM-SI data and created a fly digital embryo.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Keller/2010/Keller%202010%20Fast%20high-contrast%20imaging%20of%20animal%20development%20with%20scanned%20light%20sheet-based%20structured-illumination%20microscopy.pdf},
file = {{Keller 2010 Fast high-contrast imaging of animal development with scanned light sheet-based structured-illumination microscopy.pdf:/Users/Julien/Documents/Papers2/Articles/Keller/2010/Keller 2010 Fast high-contrast imaging of animal development with scanned light sheet-based structured-illumination microscopy.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1038/nmeth.1476}}
}

@article{Campanelli:2010jx,
author = {Campanelli, Mark and Gedeon, Tom{\'a}s},
title = {{Somitogenesis clock-wave initiation requires differential decay and multiple binding sites for clock protein}},
journal = {PLoS Computational Biology},
year = {2010},
volume = {6},
number = {4},
pages = {e1000728},
month = apr,
affiliation = {Department of Mathematics and Computer Science, Southwest Minnesota State University, Marshall, Minnesota, United States of America.},
doi = {10.1371/journal.pcbi.1000728},
pmid = {20369016},
language = {eng},
rating = {0},
date-added = {2010-10-18T08:17:46GMT+00:00},
date-modified = {2012-12-17T11:43:53GMT+00:00},
abstract = {Somitogenesis is a process common to all vertebrate embryos in which repeated blocks of cells arise from the presomitic mesoderm (PSM) to lay a foundational pattern for trunk and tail development. Somites form in the wake of passing waves of periodic gene expression that originate in the tailbud and sweep posteriorly across the PSM. Previous work has suggested that the waves result from a spatiotemporally graded control protein that affects the oscillation rate of clock-gene expression. With a minimally constructed mathematical model, we study the contribution of two control mechanisms to the initial formation of this gene-expression wave. We test four biologically motivated model scenarios with either one or two clock protein transcription binding sites, and with or without differential decay rates for clock protein monomers and dimers. We examine the sensitivity of wave formation with respect to multiple model parameters and robustness to heterogeneity in cell population. We find that only a model with both multiple binding sites and differential decay rates is able to reproduce experimentally observed waveforms. Our results show that the experimentally observed characteristics of somitogenesis wave initiation constrain the underlying genetic control mechanisms.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Campanelli/2010/Campanelli%202010%20Somitogenesis%20clock-wave%20initiation%20requires%20differential%20decay%20and%20multiple%20binding%20sites%20for%20clock%20protein.pdf},
file = {{Campanelli 2010 Somitogenesis clock-wave initiation requires differential decay and multiple binding sites for clock protein.pdf:/Users/Julien/Documents/Papers2/Articles/Campanelli/2010/Campanelli 2010 Somitogenesis clock-wave initiation requires differential decay and multiple binding sites for clock protein.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1371/journal.pcbi.1000728}}
}

@article{Manning:2010ce,
author = {Manning, M Lisa and Foty, Ramsey A and Steinberg, Malcolm S and Schoetz, Eva-Maria},
title = {{Coaction of intercellular adhesion and cortical tension specifies tissue surface tension}},
journal = {Proceedings of the National Academy of Sciences of the United States of America},
year = {2010},
volume = {107},
number = {28},
pages = {12517--12522},
month = jul,
affiliation = {Princeton Center for Theoretical Science, Princeton University, Princeton, NJ 08544, USA.},
doi = {10.1073/pnas.1003743107},
pmid = {20616053},
language = {eng},
read = {Yes},
rating = {0},
date-added = {2010-10-18T08:19:08GMT+00:00},
date-modified = {2013-01-04T20:34:28GMT+00:00},
abstract = {In the course of animal morphogenesis, large-scale cell movements occur, which involve the rearrangement, mutual spreading, and compartmentalization of cell populations in specific configurations. Morphogenetic cell rearrangements such as cell sorting and mutual tissue spreading have been compared with the behaviors of immiscible liquids, which they closely resemble. Based on this similarity, it has been proposed that tissues behave as liquids and possess a characteristic surface tension, which arises as a collective, macroscopic property of groups of mobile, cohering cells. But how are tissue surface tensions generated? Different theories have been proposed to explain how mesoscopic cell properties such as cell-cell adhesion and contractility of cell interfaces may underlie tissue surface tensions. Although recent work suggests that both may be contributors, an explicit model for the dependence of tissue surface tension on these mesoscopic parameters has been missing. Here we show explicitly that the ratio of adhesion to cortical tension determines tissue surface tension. Our minimal model successfully explains the available experimental data and makes predictions, based on the feedback between mechanical energy and geometry, about the shapes of aggregate surface cells, which we verify experimentally. This model indicates that there is a crossover from adhesion dominated to cortical-tension dominated behavior as a function of the ratio between these two quantities.},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Manning/2010/Manning%202010%20Coaction%20of%20intercellular%20adhesion%20and%20cortical%20tension%20specifies%20tissue%20surface%20tension.pdf},
file = {{Manning 2010 Coaction of intercellular adhesion and cortical tension specifies tissue surface tension.pdf:/Users/Julien/Documents/Papers2/Articles/Manning/2010/Manning 2010 Coaction of intercellular adhesion and cortical tension specifies tissue surface tension.pdf:application/pdf}},
uri = {\url{papers2://publication/livfe/id/83605}}
}

