@article{Fukazawa:2010bh,
author = {Fukazawa, Cindy and Santiago, Celine and Park, Keon Min and Deery, William J and Gomez de la Torre Canny, Sol and Holterhoff, Christopher K and Wagner, Daniel S},
title = {{poky/chuk/ikk1 is required for differentiation of the zebrafish embryonic epidermis.}},
journal = {Developmental Biology},
year = {2010},
volume = {346},
number = {2},
pages = {272--283},
month = oct,
affiliation = {Department of Biochemistry and Cell Biology, Rice University, MS-140, PO Box 1892, Houston, TX 77251, USA},
doi = {10.1016/j.ydbio.2010.07.037},
pmid = {20692251},
language = {English},
rating = {0},
date-added = {2012-09-20T17:58:44GMT+00:00},
date-modified = {2013-01-09T03:01:47GMT+00:00},
abstract = {An epidermis surrounds all vertebrates, forming a water barrier between the external environment and the internal space of the organism. In the zebrafish, the embryonic epidermis consists of an outer enveloping layer (EVL) and an inner basal layer that have distinct embryonic origins. Differentiation of the EVL requires the maternal effect gene poky/ikk1 in EVL cells prior to establishment of the basal layer. This requirement is transient and maternal Ikk1 is sufficient to allow establishment of the EVL and formation of normal skin in adults. Similar to the requirement for Ikk1 in mouse epidermis, EVL cells in poky mutants fail to exit the cell cycle or express specific markers of differentiation. In spite of the similarity in phenotype, the molecular requirement for Ikk1 is different between mouse and zebrafish. Unlike the mouse, EVL differentiation requires functioning Poky/Ikk1 kinase activity but does not require the HLH domain. Previous work suggested that the EVL was a transient embryonic structure, and that maturation of the epidermis required replacement of the EVL with cells from the basal layer. We show here that the EVL is not lost during embryogenesis but persists to larval stages. Our results show that while the requirement for poky/ikk1 is conserved, the differences in molecular activity indicate that diversification of an epithelial differentiation program has allowed at least two developmental modes of establishing a multilayered epidermis in vertebrates.},
url = {http://eutils.ncbi.nlm.nih.gov/entrez/eutils/elink.fcgi?dbfrom=pubmed&id=20692251&retmode=ref&cmd=prlinks},
local-url = {file://localhost/Users/Julien/Documents/Papers2/Articles/Fukazawa/2010/Fukazawa%202010%20pokychukikk1%20is%20required%20for%20differentiation%20of%20the%20zebrafish%20embryonic%20epidermis.pdf},
file = {{Fukazawa 2010 pokychukikk1 is required for differentiation of the zebrafish embryonic epidermis.pdf:/Users/Julien/Documents/Papers2/Articles/Fukazawa/2010/Fukazawa 2010 pokychukikk1 is required for differentiation of the zebrafish embryonic epidermis.pdf:application/pdf}},
uri = {\url{papers2://publication/doi/10.1016/j.ydbio.2010.07.037}}
}

