@Article{Gibson:2011em,
  author = {Gibson, William T and Veldhuis, James H and Rubinstein, Boris and
  Cartwright, Heather N and Perrimon, Norbert and Brodland, G Wayne and Nagpal,
  Radhika and Gibson, Matthew C},
  title = {{Control of the mitotic cleavage plane by local epithelial
  topology}},
  journal = {Cell},
  year = {2011},
  month = {February},
  volume = {144},
  number = {3},
  pages = {427--438},
  url =
  {../../papers/Articles/Gibson/2011/Gibson%202011%20Control%20of%20the%20mitotic%20cleavage%20plane%20by%20local%20epithelial%20topology.pdf},
  abstract = {For nearly 150 years, it has been recognized that cell shape
  strongly influences the orientation of the mitotic cleavage plane (e.g.,
  Hofmeister, 1863). However, we still understand little about the complex
  interplay between cell shape and cleavage-plane orientation in epithelia,
  where polygonal cell geometries emerge from multiple factors, including cell
  packing, cell growth, and cell division itself. Here, using mechanical
  simulations, we show that the polygonal shapes of individual cells can
  systematically bias the long-axis orientations of their adjacent mitotic
  neighbors. Strikingly, analyses of both animal epithelia and plant epidermis
  confirm a robust and nearly identical correlation between local cell topology
  and cleavage-plane orientation in vivo. Using simple mathematics, we show
  that this effect derives from fundamental packing constraints. Our results
  suggest that local epithelial topology is a key determinant of cleavage-plane
  orientation, and that cleavage-plane bias may be a widespread property of
  polygonal cell sheets in plants and animals.}
}

