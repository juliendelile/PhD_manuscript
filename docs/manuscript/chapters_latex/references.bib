@Article{Olivier:2010jz,
  author = {Olivier, Nicolas and Luengo-Oroz, Miguel A and Duloquin, Louise and
  Faure, Emmanuel and Savy, Thierry and Veilleux, Isra{\"e}l and Solinas,
  Xavier and D{\'e}barre, Delphine and Bourgine, Paul and Santos, Andr{\'e}s
  and Peyri{\'e}ras, Nadine and Beaurepaire, Emmanuel},
  title = {{Cell lineage reconstruction of early zebrafish embryos using
  label-free nonlinear microscopy}},
  journal = {Science},
  year = {2010},
  month = {August},
  volume = {329},
  number = {5994},
  pages = {967--971},
  url =
  {../../papers/Articles/Olivier/2010/Olivier%202010%20Cell%20lineage%20reconstruction%20of%20early%20zebrafish%20embryos%20using%20label-free%20nonlinear%20microscopy.pdf},
  abstract = {Quantifying cell behaviors in animal early embryogenesis remains
  a challenging issue requiring in toto imaging and automated image analysis.
  We designed a framework for imaging and reconstructing unstained whole
  zebrafish embryos for their first 10 cell division cycles and report
  measurements along the cell lineage with micrometer spatial resolution and
  minute temporal accuracy. Point-scanning multiphoton excitation optimized to
  preferentially probe the innermost regions of the embryo provided intrinsic
  signals highlighting all mitotic spindles and cell boundaries. Automated
  image analysis revealed the phenomenology of cell proliferation. Blastomeres
  continuously drift out of synchrony. After the 32-cell stage, the cell cycle
  lengthens according to cell radial position, leading to apparent division
  waves. Progressive amplification of this process is the rule, contrasting
  with classical descriptions of abrupt changes in the system dynamics.}
}

@Article{Oreskes:1994gn,
  author = {Oreskes, N N and Shrader-Frechette, K K and Belitz, K K},
  title = {{Verification, validation, and confirmation of numerical models in
  the Earth sciences.}},
  journal = {Science},
  year = {1994},
  month = {February},
  volume = {263},
  number = {5147},
  pages = {641--646},
  url =
  {../../papers/Articles/Oreskes/1994/Oreskes%201994%20Verification%20validation%20and%20confirmation%20of%20numerical%20models%20in%20the%20Earth%20sciences.pdf},
  abstract = {Verification and validation of numerical models of natural
  systems is impossible. This is because natural systems are never closed and
  because model results are always nonunique. Models can be confirmed by the
  demonstration of agreement between observation and prediction, but
  confirmation is inherently partial. Complete confirmation is logically
  precluded by the fallacy of affirming the consequent and by incomplete access
  to natural phenomena. Models can only be evaluated in relative terms, and
  their predictive value is always open to question. The primary value of
  models is heuristic.}
}

